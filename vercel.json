{
    "builds": [
      {
        "src": "api/app.py", 
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/upload", 
        "dest": "api/app.py"
      },
      {
        "src": "/train",  
        "dest": "api/app.py"
      },
      {
        "src": "/download-model", 
        "dest": "api/app.py"
      },
      {
        "src": "/ping",
        "dest": "api/app.py"
      },
      {
        "src": "/test", 
        "dest": "api/app.py"
      },
      {
        "src": "/(.*)", 
        "dest": "api/app.py"
      }
    ]
  }
  